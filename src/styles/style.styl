support-for-ie ?= true

/* Font Face Definitions */
@font-face
    font-family: QuicksandBold
    src: url('fonts/Quicksand_Bold.woff') format('woff'), url('fonts/Quicksand_Bold.ttf') format('truetype'), url('fonts/Quicksand_Bold.svg#QuicksandBook') format('svg');
    font-weight: normal
    font-style: normal

@font-face
    font-family: QuicksandBook
    src: url('fonts/Quicksand_Book.woff') format('woff'), url('fonts/Quicksand_Book.ttf') format('truetype'), url('fonts/Quicksand_Book.svg#QuicksandBook') format('svg');
    font-weight: normal
    font-style: normal

/* Functions */

vertical-gradient(scolor, ecolor, cstop)
    background: scolor; /* Old browsers */
    background: -moz-linear-gradient(top,  scolor 0%, ecolor cstop, scolor 100%); /* FF3.6+ */
    background: -webkit-gradient(linear, left top, left bottom, color-stop(0%,scolor), color-stop(cstop,ecolor), color-stop(100%,scolor)); /* Chrome,Safari4+ */
    background: -webkit-linear-gradient(top,  scolor 0%,ecolor cstop,scolor 100%); /* Chrome10+,Safari5.1+ */
    background: -o-linear-gradient(top,  scolor 0%,ecolor cstop,scolor 100%); /* Opera 11.10+ */
    background: -ms-linear-gradient(top,  scolor 0%,ecolor cstop,scolor 100%); /* IE10+ */
    background: linear-gradient(top,  scolor 0%,ecolor cstop,scolor 100%); /* W3C */

border-radius(n)
    -webkit-border-radius n
    -moz-border-radius n
    border-radius n

inner-shadow(size, color)
    -moz-box-shadow: inset 0 0 size color
    -webkit-box-shadow: inset 0 0 size color
    box-shadow: inset 0 0 size color

glow(size, color)
    -webkit-box-shadow:0 0 size color
    -moz-box-shadow: 0 0 size color
    box-shadow:0 0 size color

opacity(n)
  opacity n
  if support-for-ie
    filter unquote('progid:DXImageTransform.Microsoft.Alpha(Opacity=' + round(n * 100) + ')')

/* Page Defaults */
body
    margin: 0 50px 50px 50px
    padding: 0px
    font-family: QuicksandBook

a
    color: #000000
    text-decoration: none
    font-family: QuicksandBold
    
textarea
    resize: none

/* Overlays */
#mask
    position: absolute
    top: 0px
    left: 0px
    height: 100%
    width: 100%
    background: #000000
    opacity(0.75)
    z-index: 10

.pane
    z-index: 11
    position: absolute
    top: 150px
    left: 0px
    width: 100%
    display: none

    &.active
        display: block
    
    .content
        border-radius(10px)
        inner-shadow(10px, #333333)
        margin: 0 auto
        background: #ffffff
        width: 640px
        height: 480px
        color: #000000
        padding: 10px
    
#splash .content
    width: 215px
    height: 300px
    background: #ffffff url('/static/img/mascot.png') no-repeat 20px 20px
    padding: 100px 0px 0px 350px

    .message
        background: transparent url('/static/img/bubble.png') no-repeat 0px 0px
        padding: 25px 25px 0 80px
        height: 83px
        font-size: 15px


#help .content
    width: 700px
    overflow-y: auto
    font-family: arial
    
    .title
        font-size: 14px
    
    .text
        font-size: 12px
    
    ul
        margin: 0
        padding: 5px 10px
        list-style: none
    
    .shortcut
        font-size: 12px
        border: solid 1px #999999
        border-top: none
        border-collapse: collapse
        
        &:first-child
            border-top: solid 1px #999999

        .key
            border-right: solid 1px #999999
            display: inline-block
            width: 360px
            padding: 2px

        .desc
            display: inline-block
            width: 280px
            padding: 2px
    
    .command
        font-size: 12px
        border: solid 1px #999999
        border-top: none
        border-collapse: collapse
        
        &:first-child
            border-top: solid 1px #999999

        .code
            font-family: monospace
            display: inline-block
            width: 200px
            padding: 2px
            vertical-align: top

        .desc
            display: inline-block
            width: 450px
            padding: 2px
            border-left: solid 1px #999999
        
        .example
            padding: 10px
    
    .example
        font-family: monospace
        padding: 5px
        color: #666666

#samples .content
    overflow-y: auto
    font-family: arial

    .title
        font-size: 14px
        
        .copy
            font-size: 12px;
            font-family: QuicksandBook
            margin-left: 10px
            color: blue
    
    .code
        font-family: monospace
        padding: 0px 15px
        white-space: pre-line
        color: #333333


/* Main Content */
#container
    display: none

#messenger
    background: url('data:image/png;base64,') no-repeat 0px 0px
    position: absolute
    top: 6px
    left: 100px
    height: 100px
    width: 100px
    z-index: 20000

#header
    height: 40px
    padding-top: 10px
    text-align: center
    font-family: QuicksandBold
    font-size: 22px
    color: #333333
    
    .logos
        position: absolute
        right: 50px
        top: 7px
        line-height: 30px
        
        .logo
            vertical-align: middle
            display: inline
            margin-left: 5px
            
            img
                vertical-align: top
        
        div[id]
            vertical-align: text-top !important

#monitor
    background: #eaeaea
    inner-shadow(50px, #333333)
    border-radius(10px)

#buttons
    position: absolute
    right: 100px
    padding: 8px 10px
    
    ul
        padding: 0
        margin: 0
        
        .button
            margin: 5px
            padding: 5px 10px
            display: inline-block
            cursor: pointer
            border-radius(3px)
            vertical-gradient(#eaeaea, #333333, 100%)
            inner-shadow(3px, #333333)
            opacity(0.75)
            
            &:hover
                vertical-gradient(#5696f3, #ffffff, 100%)
                opacity(1.0)

#screen
    vertical-gradient(#666666, #000000, 50%)
    border: solid 3px #333333
    border-radius(5px)
    margin: 50px 50px 0px 50px
    
.multiline
    display: inline-block
    vertical-align: bottom
    border: solid 1px #cccccc
    padding-right: 30px
    cursor: pointer
    background: #ffffff url('data:image/png;base64,') no-repeat right bottom
    height: 33px
    
    input, textarea
        padding: 3px 5px 0px
        border: none
        height: 30px

    &.on
        background: #ffffff url('data:image/png;base64,') no-repeat right bottom
        position: absolute
        height: 200px
        
        input, textarea
            height: 195px
    
    &.focus
        glow(10px, #b6db49)

#input
    margin: 0 50px
    padding: 10px
    text-align: right
    
#field
    outline: none
    font-family: monospace
    font-size: 16pt

#enter
    margin: 3px 20px
    outline: none
    border: solid 1px #bbbbbb
    border-radius(2px)
    font-family: QuicksandBold
    font-size: 12pt
    padding: 5px
    -moz-box-shadow: 1px 1px 1px 1px #666;
    -webkit-box-shadow: 1px 1px 1px 1px #666;
    box-shadow: 1px 1px 1px 1px #666;

    &:focus, &:hover
        glow(10px, #b6db49)

#error
    display: none
