extends layout.pug
block body
    div#fb-root
    script.
        (function(d, s, id) {
        var js, fjs = d.getElementsByTagName(s)[0];
        if (d.getElementById(id)) return;
        js = d.createElement(s); js.id = id;
        js.src = "//connect.facebook.net/en_US/all.js#xfbml=1";
        fjs.parentNode.insertBefore(js, fjs);
        }(document, 'script', 'facebook-jssdk'));
    div#overlay
        div#mask
        div#splash.pane.active
            div.content
                div.message Ready to draw? Just a minute while I gather my stuff!!
        div#help.pane
            div.content
                include help
        div#samples.pane
            div.content
                include samples
    div#container
        div#messenger
        div#header Turtle Graphics
            div.logos
                div.logo
                    a(href='http://www.w3.org/html/logo/')
                        img(height='30', alt="HTML5 Powered with CSS3 / Styling, Performance &amp; Integration, and Offline &amp; Storage", title="HTML5 Powered with CSS3 / Styling, Performance &amp; Integration, and Offline &amp; Storage", src="data:image/png;qs=07;base64,")
                div.logo.g-plusone(data-size="medium", data-annotation="none", data-href="http://logo.dodla.com")
                script.
                    (function() {
                        var po = document.createElement('script'); po.type = 'text/javascript'; po.async = true;
                        po.src = 'https://apis.google.com/js/plusone.js';
                        var s = document.getElementsByTagName('script')[0]; s.parentNode.insertBefore(po, s);
                    })();
                div.logo.fb-like(data-href='http://logo.dodla.com', data-send='false', data-layout='button_count', data-width='100', data-show-faces='true')
        div#monitor
            div#buttons
                ul
                    li.button.tipper(tip='Need some help?'): a(href='#help') Help
                    li.button.tipper(tip='Wanna see some sample code?'): a(href='#samples') Samples
            canvas#screen
            div#input
                div.multiline: textarea#field.tipper(tip='Your code goes here!!', autofocus='autofocus')
                input#enter.tipper(type='button', value='Enter', tip='Click to execute!!')
                span#error
